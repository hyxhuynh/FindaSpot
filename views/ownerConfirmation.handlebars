<form id="regForm" action="/googleMapsPage">
    <h4>Summary</h4>
    <!-- One "tab" for each step in the form: -->
    <!-- FIRST TAB -->
    <div class="tab">
        <p>
            <strong>Address:</strong> {{address}}, {{city}}, {{state}} {{postalCode}}</p>
        <p>
        <p>
            <strong>Price:</strong> {{price}}
        </p>
        <p>
            <strong>Type:</strong> {{spaceCover}}
        </p>
        <p>
            <strong>Size:</strong> {{spaceSize}}
        </p>
        <p>
            <strong>Description:</strong> {{description}}
        </p>
        <p>
            Driver can reach at the following contact information:
        </p>
        <p>
            Name: {{ownerName}}
        </p>
        <p>
            Phone: {{ownerPhone}}
        </p>
        <p>
            Email: {{ownerEmail}}
        </p>
        
    </div>

    <div style="overflow:auto;">
        <div style="float:right;">
            <button><a href="/">‚Üê Back To Home</a></button>
        <button type="button" id="prevBtn" onclick="nextPrev(-1)">Previous</button>
        <button type="button" id="nextBtn" onclick="nextPrev(1)">Next</button>
        </div>
    </div>


    <!-- Circles which indicates the steps of the form: -->
    <div style="text-align:center;margin-top:40px;">
        <span class="step"></span>
    </div>
</form>

