<form id="regForm" method="POST" action="/api/parkingspace">
    <h4>List Your Parking Space</h4>
    <!-- One "tab" for each step in the form: -->
    <!-- FIRST TAB -->
    <div class="tab">
        <h5>Enter Your Address</h5>

        <!-- address input-->
        <div class="control-group">
                <label class="control-label">Address</label>
                <div class="controls">
                    <input id="autocomplete" id="address" name="address" type="text" placeholder="address line 1"
                    class="input-xlarge">
                    <p class="help-block">Street address, P.O. box, company name, c/o</p>
                </div>
            </div>

            <!-- city input-->
            <div class="control-group">
                <label class="control-label">City / Town</label>
                <div class="controls">
                    <input id="city" name="city" type="text" placeholder="city" class="input-xlarge">
                    <p class="help-block"></p>
                </div>
            </div>
            <!-- region input-->
            <div class="control-group">
                <label class="control-label">State / Province / Region</label>
                <div class="controls">
                    <input id="state" name="state" type="text" placeholder="state / province / region"
                    class="input-xlarge">
                    <p class="help-block"></p>
                </div>
            </div>
            <!-- postal-code input-->
            <div class="control-group">
                <label class="control-label">Zip / Postal Code</label>
                <div class="controls">
                    <input id="postalCode" name="postalCode" type="text" placeholder="zip or postal code"
                    class="input-xlarge">
                    <p class="help-block"></p>
                </div>
            </div>
    </div>
    <!-- SECOND TAB -->
    <div class="tab">
        <h5>Parking Location</h5>
        <p>
            Customer will go to your posted location. Adjust the pin to match your parking space exact location.
        </p>

    </div>
    <!-- THIRD TAB -->
    <div class="tab">
            <h5>Parking Space</h5>
            <p>How much is the cost per day?<input placeholder="price" name="price" ></p>
            <p>Which type of parking space do you have?<input placeholder="uncovered, covered, garage" name="spaceCover" ></p>
            <p>What size is your parking space?<input placeholder="standard,compact, motorcycle, rv" name="spaceSize" ></p>
            <p>Additional description of the Parking Space:<input placeholder="Type here" name="description" ></p>
    </div>
    <!-- FOURTH TAB -->
    <div class="tab">
        <h5>Contact Information</h5>
        <p><input placeholder="Your Name" name="ownerName" ></p>
        <p><input placeholder="Phone Number" name="ownerPhone" ></p>
        <p><input placeholder="Email Address" name="ownerEmail" ></p>
    </div>
    <div style="overflow:auto;">
        <div style="float:right;">
        <button type="button" id="prevBtn" onclick="nextPrev(-1)">Previous</button>
        <button type="button" id="nextBtn" onclick="nextPrev(1)">Next</button>
        </div>
    </div>
    <!-- Circles which indicates the steps of the form: -->
    <div style="text-align:center;margin-top:40px;">
        <span class="step"></span>
        <span class="step"></span>
        <span class="step"></span>
        <span class="step"></span>
    </div>
</form>


