<form id="regForm" class="regForm" method="POST" action="/api/parkingspace">
    <h4>List Your Parking Space</h4>
    <!-- One "tab" for each step in the form: -->
    <!-- FIRST TAB -->
    <div class="tab">
        <h5>Enter Your Address</h5>

        <!-- address input-->
        <div class="control-group">
            {{!-- <label class="control-label">Address</label> --}}
            <div class="controls">
                <input id="autocomplete" id="address" name="address" type="text" placeholder="address"
                class="input-xlarge">
            </div>
        </div>
    </div>

    <!-- SECOND TAB -->
   {{!-- <div class="tab dateTimeContainer">
        <div class="dateTime">
