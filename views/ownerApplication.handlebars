<form id="regForm" method="POST" action="/api/parkingspace">
    <h4>List Your Parking Space</h4>
    <!-- One "tab" for each step in the form: -->
    <!-- FIRST TAB -->
    <div class="tab">
        <h5>Enter Your Address</h5>

        <!-- address input-->
        <div class="control-group">
            <label class="control-label">Address</label>
            <div class="controls">
                <input id="autocomplete" id="address" name="address" type="text" placeholder="address line 1"
                class="input-xlarge">
                <p class="help-block">Street address, P.O. box, company name, c/o</p>
            </div>
        </div>
    </div>

    {{!-- <!-- SECOND TAB -->
    <div class="tab">
        <h5>Parking Location</h5>
        <p>
            Customer will go to your posted location. Adjust the pin to match your parking space exact location.
        </p>
    </div> --}}

    <!-- THIRD TAB -->
    <div class="tab">
            <h5>Parking Space</h5>
            <p>How much is the cost per day?<input placeholder="price" name="price" ></p>

            <p>
                Which type of parking space do you have?
                <label><input type="radio" name="spaceCover" value="uncovered"><span>Uncovered</span></label>
                <label><input type="radio" name="spaceCover" value="covered"><span>Covered</span></label>
                <label><input type="radio" name="spaceCover" value="garage"><span>Garage</span></label>
            </p>

            <p>
                What size is your parking space?
                <label><input type="radio" name="spaceSize" value="standard"><span>Standard</span></label>
                <label><input type="radio" name="spaceSize" value="compact"><span>Compact</span></label>
                <label><input type="radio" name="spaceSize" value="motorcycle"><span>Motorcycle</span></label>
                <label><input type="radio" name="spaceSize" value="rv"><span>RV</span></label>
            </p>
            <p>Additional description of the Parking Space:<input placeholder="Type here" name="description" ></p>
    </div>
    <!-- FOURTH TAB -->
    <div class="tab">
        <h5>Contact Information</h5>
        <p><input placeholder="Your Name" name="ownerName" ></p>
        <p><input placeholder="Phone Number" name="ownerPhone" ></p>
        <p><input placeholder="Email Address" name="ownerEmail" ></p>
    </div>
    <div style="overflow:auto;">
        <div style="float:right;">
        <button type="button" id="prevBtn" onclick="nextPrev(-1)">Previous</button>
        <button type="button" id="nextBtn" onclick="nextPrev(1)">Next</button>
        </div>
    </div>
    <!-- Circles which indicates the steps of the form: -->
    <div style="text-align:center;margin-top:40px;">
        <span class="step"></span>
        <span class="step"></span>
        <span class="step"></span>
        <span class="step"></span>
    </div>
</form>


